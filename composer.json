{
  "name": "[composer_vendor]/[composer_name]",
  "description": "[plugin_description]",
  "type": "wordpress-plugin",
  "keywords": [
    "wordpress",
    "plugin"
  ],
  "homepage": "[plugin_url]",
  "license": "GPL-2.0+",
  "require": {
    "composer/installers": "~1.0",
    "dg/composer-cleaner": "^1.1",
    "php": ">=7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0.0",
    "10up/wp_mock": "@dev"
  },
  "authors": [],
  "autoload": {
    "psr-4": {
      "[autoload_psr_4]": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "[autoload_tests_psr_4]": "tests/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "sort-packages": true
  }
}
